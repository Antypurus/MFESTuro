class BikeRack is subclass of Feature
values
	private NAME: string = "Bike Rack";
	private DESCRIPTION: string = "This car has a bike rack";
	private VALUE: real = 1.0;
operations
	public BikeRack: () ==> BikeRack
	BikeRack() ==
	(
		name := NAME;
		description := DESCRIPTION;
		value := VALUE;
		return self;
	);
end BikeRack