class HeatedSeats is subclass of Feature
values
	private NAME: string = "Heated Seat";
	private DESCRIPTION: string = "This car has heated seats";
	private VALUE: real = 1.0;
operations
	public HeatedSeats: () ==> HeatedSeats
	HeatedSeats() ==
	(
		name := NAME;
		description := DESCRIPTION;
		value := VALUE;
		return self;
	);
end HeatedSeats