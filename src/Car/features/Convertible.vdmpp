class Convertible is subclass of Feature
values
	private NAME: string = "Convertible";
	private DESCRIPTION: string = "This car is a convertible";
	private VALUE: real = 1.0;
operations
	public Convertible: () ==> Convertible
	Convertible() ==
	(
		name := NAME;
		description := DESCRIPTION;
		value := VALUE;
		return self;
	);
end Convertible